  abstract = {Invariant item ordering refers to the statement that if one item is harder than another for one person, then it is harder for all people. Whether item ordering holds is a psychological statement because it describes how people may qualitatively vary. Yet, modern item response theory (IRT) makes an a priori commitment to item ordering. The Rasch model, for example, posits that items must order. Conversely, the 2PL model posits that items never order. Needed is an IRT model where item ordering or its violation is a function of the data rather than an a priori commitment. We develop two-parameter shift-scale models for this purpose, and find that the two-parameter uniform offers many advantages. We show how item ordering may be assessed using Bayes factor model comparison, and discuss computational issues with shift-scale IRT models.},
  langid = {english},
  keywords = {Cognitive psychometrics,Item ordering,Item response theory},
  file = {/Users/jeff/Zotero/storage/UM3Z92LW/Haaf et al. - 2020 - Do items order The psychology in IRT models.pdf;/Users/jeff/Zotero/storage/SL4CZKEC/S0022249620300572.html}
}

@article{Haaf.etal.2024,
  title = {Attentional {{Control Data Collection}}: {{A Resource}} for {{Efficient Data Reuse}}},
  shorttitle = {Attentional {{Control Data Collection}}},
  author = {Haaf, Julia M. and Hoffstadt, Madlen and Lesche, Sven},
  date = {2024-02-23},
  publisher = {OSF},
  doi = {10.31234/osf.io/4evy6},
  url = {https://osf.io/4evy6},
  urldate = {2024-03-07},
  abstract = {Publicly available data are required 1. to assess the reproducibility of each individual finding in the literature, and 2. to promote the reuse of data for a more efficient use of participants' time and public resources. Current data sharing efforts are well suited for the first goal; yet, they do not sufficiently address the second goal. Here, we show how structured collections of open data can be useful as they allow a larger community of researchers easy access to a large body of data from their own research area. We introduce the Attentional Control Data Collection, an SQL data base for attentional control experiments. We illustrate the structure of the data base, how it can be easily accessed using a shiny app and an R-package, and how researchers can contribute data from their studies to the data base. Finally, we conduct our own initial analysis of the 64 data sets in our data base, assessing the reliability of individual differences. The analysis highlights that reliability is generally low, and provides insights into planning future studies. For example researchers should consider to increase the number of trials per person and condition to at least 400. The analysis highlights how an open data base like ACDC can aid meta-analytic efforts as well as methodological innovation.},
  langid = {american},
  file = {/Users/jeff/Zotero/storage/U2L9HDNG/Haaf et al. - 2024 - Attentional Control Data Collection A Resource fo.pdf;/Users/jeff/Zotero/storage/MRGVGLL8/4evy6.html}
}

