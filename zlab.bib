@article{Gu.etal.2016,
  title = {A Hierarchical {{Bayesian}} Approach to Adaptive Vision Testing: {{A}} Case Study with the Contrast Sensitivity Function},
  shorttitle = {A Hierarchical {{Bayesian}} Approach to Adaptive Vision Testing},
  author = {Gu, Hairong and Kim, Woojae and Hou, Fang and Lesmes, Luis Andres and Pitt, Mark A. and Lu, Zhong-Lin and Myung, Jay I.},
  date = {2016-04-21},
  journaltitle = {Journal of Vision},
