@article{Corbin.Marquer.2013,
  title = {Is {{Sternberg}}’s {{Memory Scanning Task Really}} a {{Short-Term Memory Task}}?},
  author = {Corbin, Lucie and Marquer, Josette},
  date = {2013-01},
  journaltitle = {Swiss Journal of Psychology},
  volume = {72},
