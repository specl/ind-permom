@article{VanMaanen.etal.2012,
  title = {Piéron's Law and Optimal Behavior in Perceptual Decision-Making},
  author = {Van Maanen, Leendert and Grasman, Raoul P. and Forstmann, Birte U. and Wagenmakers, Eric-Jan},
  date = {2012-01-02},
  journaltitle = {Frontiers in Neuroscience},
  shortjournal = {Front. Neurosci.},
  volume = {5},
